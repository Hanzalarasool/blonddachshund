@model Resume

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Resume</h4>
    <hr />
    <dl class="row">

        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Company)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Company)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ResumeExperiences)
        </dt>
        <dd class="col-sm-10">
            <table class="table">
                <tr>
                    <th>B One</th>
                </tr>
                @foreach (var item in Model.ResumeExperiences)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Experience.Item)
                        </td>
                    </tr>
                }
            </table>
        </dd>
 
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
